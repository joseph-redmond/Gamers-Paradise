<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />
    <title>@ViewData["Title"] - gaming_full_stack_website</title>
	<link rel="stylesheet" type="text/css" href="/css/bootstrap/bootstrap.min.css">
    <link href="/images/favicon/favicon-16x16.png" rel="shortcut icon">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" type="text/css" href="/css/community/community.css">
    <link href="/css/site.css" rel="stylesheet" />
</head>

<body class="loading">
        <partial name="_TopMenu" />   
   
        <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <partial name="_Footer" />
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - gaming_full_stack_website - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <script src="/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/js/main.js"></script>
    <script src="/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
